{
  "$schema": "https://raw.githubusercontent.com/OpenLightingProject/open-fixture-library/master/schemas/fixture.json",
  "name": "Michi LED",
  "shortName": "MLED",
  "categories": ["Color Changer"],
  "meta": {
    "authors": ["Teichi"],
    "createDate": "2019-10-23",
    "lastModifyDate": "2019-10-23"
  },
  "availableChannels": {
    "Dimmer": {
      "capability": {
        "type": "Intensity"
      }
    },
    "CTB": {
      "capability": {
        "type": "ColorIntensity",
        "color": "CTB"
      }
    },
    "White": {
      "capability": {
        "type": "ColorIntensity",
        "color": "White"
      }
    },
    "Strobe": {
      "capability": {
        "type": "ShutterStrobe",
        "shutterEffect": "Strobe",
        "speedStart": "0.1Hz",
        "speedEnd": "20Hz"
      }
    }
  },
  "modes": [
    {
      "name": "8 bit",
      "shortName": "8bit",
      "channels": [
        "Dimmer",
        "Strobe",
        "CTB",
        "White"
      ]
    }
  ]
}
